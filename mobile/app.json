{
  "expo": {
    "name": "Bookshelf Scanner Mobile",
    "slug": "bookshelf-scanner-mobile",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Use the camera to scan books on your shelf."
      }
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "plugins": [
      "expo-camera"
    ]
  }
}
